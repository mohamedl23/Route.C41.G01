@model Employee 
@{
    ViewData["Title"] = "Create Employee";
}

<h1 class="pb-4">Create New Employee </h1>
<form asp-action="Create" class="form-group" >
    
    <partial name="PartialView/CreateEditEmployee" />
    
    <input type="submit" class="btn btn-primary" value="Create" /> |
    <a asp-action="Index" class="btn btn-light" >Back To List</a>
</form>


@section ValidationScriptsSection {
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
}